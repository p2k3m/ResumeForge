{
  "name": "resumeforge",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "dev": "node server.js",
    "copy-fonts": "if [ -d fonts ]; then mkdir -p dist/fonts && cp -r fonts/* dist/fonts/; fi",
    "build": "npm run copy-fonts && echo 'No build step'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.432.0",
    "@aws-sdk/client-secrets-manager": "^3.432.0",
    "@aws-sdk/client-dynamodb": "^3.432.0",
    "@aws-sdk/s3-request-presigner": "^3.432.0",
    "@google/generative-ai": "^0.11.3",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "docx": "^8.0.2",
    "express": "^4.18.2",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.14.0",
    "puppeteer": "^22.9.0",
    "json5": "^2.2.3",
    "openai": "^4.0.0",
    "ua-parser-js": "^1.0.35",
    "fontkit": "^1.9.0",
    "qrcode": "^1.5.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "babel-jest": "^29.7.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.jsx?$": ["babel-jest", {"presets": ["@babel/preset-env", "@babel/preset-react"]}]
    },
    "moduleNameMapper": {
      "^@google/generative-ai$": "<rootDir>/tests/mocks/google-generative-ai.js",
      "^@aws-sdk/client-dynamodb$": "<rootDir>/tests/mocks/aws-sdk-client-dynamodb.js",
      "^@aws-sdk/s3-request-presigner$": "<rootDir>/tests/mocks/aws-sdk-s3-request-presigner.js",
      "^openai$": "<rootDir>/tests/mocks/openai.js"
    }
  }
}
