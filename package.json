{
  "name": "resumeforge",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "dev": "node server.js",
    "clean": "node scripts/clean.mjs",
    "prebuild:all": "npm run clean",
    "build": "npm run build:client && npm run build:lambda",
    "build:client": "node scripts/build-client.mjs",
    "build:lambda": "node scripts/build-lambda.mjs",
    "build:all": "npm run build",
    "lint": "eslint . --ext .js,.jsx,.mjs --no-report-unused-disable-directives",
    "fix": "eslint . --ext .js,.jsx,.mjs --fix",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --testPathIgnorePatterns='\\.integration\\.test\\.js$' --testPathIgnorePatterns='\\.e2e\\.test\\.js$'",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --testPathPattern='\\.integration\\.test\\.js$'",
    "test:api": "npm run test:integration",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --testPathPattern='\\.e2e\\.test\\.js$'",
    "test:templates": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand tests/templateSections.test.js tests/pdfTemplateBackstop.test.js tests/uploadFormatsTemplates.e2e.test.js",
    "test:ci": "npm run lint && npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:templates",
    "verify": "npm run test:ci",
    "print:cloudfront-url": "node scripts/print-cloudfront-url.mjs",
    "refresh:templates": "node scripts/refresh-templates.mjs",
    "predeploy:sam": "npm run build:client && node scripts/ensure-client-dist.mjs",
    "deploy:sam": "sam build --cached --parallel && sam deploy",
    "test:sam": "sam local invoke AuditingFunction --event tests/sam/healthz-event.json",
    "publish:cloudfront-url": "node scripts/publish-cloudfront-url.mjs",
    "upload:static": "node scripts/upload-static-build.mjs",
    "verify:cloudfront": "node scripts/verify-cloudfront.mjs",
    "verify:static": "node scripts/verify-static-deployment.mjs",
    "repair:cloudfront": "node scripts/repair-cloudfront-assets.mjs",
    "watch:cloudfront": "node scripts/watch-cloudfront-assets.mjs",
    "autofix:pipeline": "node scripts/auto-pipeline-repair.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.432.0",
    "@aws-sdk/client-cloudfront": "^3.899.0",
    "@aws-sdk/client-cloudwatch": "^3.911.0",
    "@aws-sdk/client-dynamodb": "^3.432.0",
    "@aws-sdk/client-eventbridge": "^3.908.0",
    "@aws-sdk/client-lambda": "^3.917.0",
    "@aws-sdk/client-s3": "^3.432.0",
    "@aws-sdk/client-secrets-manager": "^3.432.0",
    "@aws-sdk/client-sqs": "^3.432.0",
    "@aws-sdk/lib-storage": "^3.911.0",
    "@aws-sdk/s3-request-presigner": "^3.432.0",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@google/generative-ai": "^0.11.3",
    "@octokit/rest": "^22.0.0",
    "@vendia/serverless-express": "^4.10.3",
    "axios": "^1.6.2",
    "cfb": "^1.2.2",
    "cors": "^2.8.5",
    "docx": "^8.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "json5": "^2.2.3",
    "jszip": "^3.10.1",
    "mammoth": "^1.6.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "openai": "^6.7.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.13.0",
    "qrcode": "^1.5.4",
    "ua-parser-js": "^1.0.35",
    "whatwg-mimetype": "^3.0.0",
    "word-extractor": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "babel-jest": "^29.7.0",
    "chokidar": "^4.0.3",
    "esbuild": "^0.20.2",
    "eslint": "^9.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-yaml": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "selenium-webdriver": "^4.38.0",
    "supertest": "^6.3.4",
    "undici": "^5.29.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/tests/setupEnv.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@google/generative-ai$": "<rootDir>/tests/mocks/google-generative-ai.js",
      "^@aws-sdk/client-dynamodb$": "<rootDir>/tests/mocks/aws-sdk-client-dynamodb.js",
      "^@aws-sdk/s3-request-presigner$": "<rootDir>/tests/mocks/aws-sdk-s3-request-presigner.js",
      "^@vendia/serverless-express$": "<rootDir>/tests/mocks/serverless-express.js",
      "^puppeteer$": "<rootDir>/tests/mocks/puppeteer.js",
      "^handlebars$": "<rootDir>/tests/mocks/handlebars.js",
      "^qrcode$": "<rootDir>/tests/mocks/qrcode.js",
      "^react$": "<rootDir>/node_modules/react/index.js",
      "^react-dom$": "<rootDir>/node_modules/react-dom/index.js",
      "coverTemplateStyles\\.js$": "<rootDir>/client/src/utils/coverTemplateStyles.cjs",
      "actionableAdvice\\.js$": "<rootDir>/client/src/utils/actionableAdviceShared.cjs",
      "templateRegistry\\.js$": "<rootDir>/client/src/templateRegistry.cjs",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "server.js",
      "logger.js",
      "lib/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "bail": true,
    "forceExit": true
  }
}
