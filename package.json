{
  "name": "resumeforge",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "dev": "node server.js",
    "build": "npm run build:client && npm run build:lambda",
    "build:client": "npm run build --prefix client",
    "build:lambda": "node scripts/build-lambda.mjs",
    "print:cloudfront-url": "node scripts/print-cloudfront-url.mjs",
    "test:sam": "sam local invoke ResumeForgeFunction --event tests/sam/healthz-event.json",
    "publish:cloudfront-url": "node scripts/publish-cloudfront-url.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.432.0",
    "@aws-sdk/client-cloudfront": "^3.899.0",
    "@aws-sdk/client-dynamodb": "^3.432.0",
    "@aws-sdk/client-s3": "^3.432.0",
    "@aws-sdk/client-secrets-manager": "^3.432.0",
    "@aws-sdk/s3-request-presigner": "^3.432.0",
    "@google/generative-ai": "^0.11.3",
    "@vendia/serverless-express": "^4.10.3",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "docx": "^8.0.2",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "json5": "^2.2.3",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.13.0",
    "qrcode": "^1.5.4",
    "ua-parser-js": "^1.0.35",
    "word-extractor": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.24.5",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.20.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/tests/setupEnv.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@google/generative-ai$": "<rootDir>/tests/mocks/google-generative-ai.js",
      "^@aws-sdk/client-dynamodb$": "<rootDir>/tests/mocks/aws-sdk-client-dynamodb.js",
      "^@aws-sdk/s3-request-presigner$": "<rootDir>/tests/mocks/aws-sdk-s3-request-presigner.js",
      "^@vendia/serverless-express$": "<rootDir>/tests/mocks/serverless-express.js",
      "^puppeteer$": "<rootDir>/tests/mocks/puppeteer.js",
      "^handlebars$": "<rootDir>/tests/mocks/handlebars.js",
      "^qrcode$": "<rootDir>/tests/mocks/qrcode.js",
      "^react$": "<rootDir>/node_modules/react/index.js",
      "^react-dom$": "<rootDir>/node_modules/react-dom/index.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "server.js",
      "logger.js",
      "lib/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "bail": true,
    "forceExit": true
  }
}
